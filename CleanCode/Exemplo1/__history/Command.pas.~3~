unit Command;

interface

uses
  Interfaces;

type
  TCommand = class(TInterfacedObject, iCommand)
    private
      FResolveString : String;
    public
      constructor Create;
      destructor Destroy; override;
      class function New : iCommand;
      function resolve ( aValue : String ) : iCommand;
  end;

implementation

{ TCommand }

constructor TCommand.Create;
begin

end;

destructor TCommand.Destroy;
begin

  inherited;
end;

class function TCommand.New: iCommand;
begin
  Result := Self.Create;
end;

function TCommand.resolve(aValue: String): iCommand;
begin
  Result := Self;
  FResolveString := aValue;
end;

end.
